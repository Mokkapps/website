  "scripts": {
    "start": "gatsby develop -p 8000",
    "prebuild": "npm run preload-fonts",
    "build": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "serve": "gatsby serve",
    "serve-e2e": "npm run build && gatsby serve -p 8000",
    "start-server": "gatsby develop",
    "generate-blogpost": "node generate/blogpost.js",
    "cy:open": "cypress open",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "test:e2e:ci": "start-server-and-test serve-e2e http://localhost:8000 cy:run",
    "test:e2e": "start-server-and-test start http://localhost:8000 cy:open",
    "lint": "eslint src",
    "lighthouse:local": "lighthouse http://localhost:8000  --output html --output-path .lighthouse-report.html",
    "lighthouse:prod": "lighthouse https://mokkapps.de  --output html --output-path .lighthouse-report.html",
    "preload-fonts": "gatsby-preload-fonts"
  },
